{
  "name": "grafite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-https": "next dev --experimental-https --port 443",
    "build": "next build",
    "clean": "rm -rf .next",
    "start": "next start",
    "check": "prettier --check src && eslint src && tsc --noEmit -p tsconfig.json && stylelint src/**/*.css src/**/*.scss",
    "fix:imports": "eslint --fix --rule '{\"simple-import-sort/imports\":\"error\",\"simple-import-sort/exports\":\"error\"}'",
    "fix:staged": "lint-staged src",
    "fix": "prettier --log-level silent --write src && eslint --fix src && stylelint --fix=lax src/**/*.css src/**/*.scss",
    "test": "jest"
  },
  "dependencies": {
    "@carbon/charts-react": "^1.27.0",
    "@carbon/react": "^1.95.0",
    "@streamparser/json-whatwg": "^0.0.21",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "ag-grid-community": "^33.1.1",
    "ag-grid-react": "^33.3.2",
    "amqplib": "^0.10.9",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^14.2.33",
    "next-auth": "^4.24.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-json-tree": "^0.20.0",
    "react-markdown": "^9.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-textarea-autosize": "^8.5.9",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sass": "^1.94.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
